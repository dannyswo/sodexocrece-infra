{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "eastus2"
    },
    "env": {
      "value": "PRD"
    },
    "aksSkuTier": {
      "value": "Paid"
    },
    "aksDnsSuffix": {
      "value": "clp885"
    },
    "kubernetesVersion": {
      "value": "1.23.12"
    },
    "nodeResourceGroupName": {
      "value": "{NULL nodeResourceGroupName}"
    },
    "subnetName": {
      "value": "{NULL subnetName}"
    },
    "enableAutoScaling": {
      "value": true
    },
    "minCount": {
      "value": 2
    },
    "maxCount": {
      "value": 4
    },
    "vmSize": {
      "value": "standard_d2s_v3"
    },
    "enableEncryptionAtHost": {
      "value": true
    },
    "applicationGatewayName": {
      "value": "{NULL applicationGatewayName}"
    },
    "logAnalyticsWorkspaceName": {
      "value": "{NULL logAnalyticsWorkspaceName}"
    }
  }
}